{
  "name": "unsplash-challenge",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.2",
    "prettier": "^1.16.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --single-quote --no-semi --trailing-comma es5 --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
